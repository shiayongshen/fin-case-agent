[
  {
    "id": "insurance:violate_148_1_1_or_2",
    "desc": "違反第一百四十八條之一第一項或第二項規定",
    "expr": [
      "EQ",
      "violate_148_1_1_or_2",
      [
        "OR",
        [
          "EQ",
          "violate_148_1_1",
          true
        ],
        [
          "EQ",
          "violate_148_1_2",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violate_148_2_1",
    "desc": "違反第一百四十八條之二第一項規定：未提供說明文件供查閱、或所提供之說明文件未依規定記載、或所提供之說明文件記載不實",
    "expr": [
      "EQ",
      "violate_148_2_1",
      [
        "OR",
        [
          "EQ",
          "not_provide_explanation_doc",
          true
        ],
        [
          "EQ",
          "explanation_doc_not_according_to_rule",
          true
        ],
        [
          "EQ",
          "explanation_doc_false",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violate_148_2_2",
    "desc": "違反第一百四十八條之二第二項規定：未依限向主管機關報告或主動公開說明，或向主管機關報告或公開說明之內容不實",
    "expr": [
      "EQ",
      "violate_148_2_2",
      [
        "OR",
        [
          "EQ",
          "not_report_to_authority_in_time",
          true
        ],
        [
          "EQ",
          "not_proactively_disclose",
          true
        ],
        [
          "EQ",
          "report_or_disclosure_false",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violate_148_3_1",
    "desc": "違反第一百四十八條之三第一項規定：未建立或未執行內部控制或稽核制度",
    "expr": [
      "EQ",
      "violate_148_3_1",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "internal_control_established",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_control_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violate_148_3_2",
    "desc": "違反第一百四十八條之三第二項規定：未建立或未執行內部處理制度或程序",
    "expr": [
      "EQ",
      "violate_148_3_2",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "internal_handling_established",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_handling_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：違反任一相關規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "violate_148_1_1_or_2",
          true
        ],
        [
          "EQ",
          "violate_148_2_1",
          true
        ],
        [
          "EQ",
          "violate_148_2_2",
          true
        ],
        [
          "EQ",
          "violate_148_3_1",
          true
        ],
        [
          "EQ",
          "violate_148_3_2",
          true
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
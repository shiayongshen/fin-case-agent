[
  {
    "id": "insurance:capital_level",
    "desc": "保險業資本等級分類（1=資本適足, 2=資本不足, 3=資本顯著不足, 4=資本嚴重不足, 0=未分類）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "OR",
          [
            "AND",
            [
              "GE",
              "capital_adequacy_ratio",
              150.0
            ],
            [
              "LT",
              "capital_adequacy_ratio",
              200.0
            ]
          ],
          [
            "AND",
            [
              "LT",
              "net_worth_ratio",
              2.0
            ],
            [
              "GE",
              "net_worth_ratio",
              0.0
            ]
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_lowest",
    "desc": "資本等級以較低等級為準（同時符合多等級時）",
    "expr": [
      "EQ",
      "capital_level_lowest",
      [
        "CASE",
        [
          "EQ",
          "capital_level",
          4
        ],
        4,
        [
          "EQ",
          "capital_level",
          3
        ],
        3,
        [
          "EQ",
          "capital_level",
          2
        ],
        2,
        [
          "EQ",
          "capital_level",
          1
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures_submitted",
    "desc": "資本不足者已提出增資、財務或業務改善計畫",
    "expr": [
      "EQ",
      "capital_insufficient_measures_submitted",
      [
        "EQ",
        "improvement_plan_submitted",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures_executed",
    "desc": "資本不足者已依計畫確實執行改善措施",
    "expr": [
      "EQ",
      "capital_insufficient_measures_executed",
      [
        "EQ",
        "improvement_plan_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures_ok",
    "desc": "資本不足者已提出且執行改善計畫",
    "expr": [
      "EQ",
      "capital_insufficient_measures_ok",
      [
        "AND",
        [
          "EQ",
          "capital_insufficient_measures_submitted",
          true
        ],
        [
          "EQ",
          "capital_insufficient_measures_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient_measures_ok",
    "desc": "資本顯著不足者已採取主管機關規定之措施",
    "expr": [
      "EQ",
      "capital_significantly_insufficient_measures_ok",
      [
        "EQ",
        "significantly_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient_measures_ok",
    "desc": "資本嚴重不足者已採取主管機關規定之處分措施",
    "expr": [
      "EQ",
      "capital_severely_insufficient_measures_ok",
      [
        "EQ",
        "severely_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：資本不足等級達一定程度且未執行對應措施時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "AND",
          [
            "EQ",
            "capital_level",
            4
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_severely_insufficient_measures_ok",
              true
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level",
            3
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_significantly_insufficient_measures_ok",
              true
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level",
            2
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_insufficient_measures_ok",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
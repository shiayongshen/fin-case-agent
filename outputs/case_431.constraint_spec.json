[
  {
    "id": "insurance:capital_level",
    "desc": "資本等級分類（1=適足, 2=不足, 3=顯著惡化, 4=嚴重不足）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_4_noncompliance",
    "desc": "資本嚴重不足且未於期限完成增資或改善計畫或合併",
    "expr": [
      "EQ",
      "capital_level_4_noncompliance",
      [
        "AND",
        [
          "EQ",
          "capital_level",
          4
        ],
        [
          "NOT",
          [
            "EQ",
            "capital_level_4_compliance",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_4_compliance",
    "desc": "資本嚴重不足等級完成增資、財務或業務改善計畫或合併",
    "expr": [
      "EQ",
      "capital_level_4_compliance",
      [
        "EQ",
        "capital_level_4_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:significant_deterioration",
    "desc": "財務或業務狀況顯著惡化且不能支付債務或履行契約或有損及被保險人權益之虞",
    "expr": [
      "EQ",
      "significant_deterioration",
      [
        "AND",
        [
          "EQ",
          "financial_or_business_deterioration",
          true
        ],
        [
          "OR",
          [
            "EQ",
            "unable_to_pay_debt",
            true
          ],
          [
            "EQ",
            "unable_to_fulfill_contract",
            true
          ],
          [
            "EQ",
            "risk_to_insured_interest",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_submitted_and_approved",
    "desc": "已提出財務或業務改善計畫並經主管機關核定",
    "expr": [
      "EQ",
      "improvement_plan_submitted_and_approved",
      [
        "AND",
        [
          "EQ",
          "improvement_plan_submitted",
          true
        ],
        [
          "EQ",
          "improvement_plan_approved",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_accelerated_deterioration",
    "desc": "損益、淨值加速惡化且經輔導仍未改善",
    "expr": [
      "EQ",
      "improvement_plan_accelerated_deterioration",
      [
        "AND",
        [
          "EQ",
          "profit_loss_accelerated_deterioration",
          true
        ],
        [
          "NOT",
          [
            "EQ",
            "improvement_plan_effective",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:supervisory_measures_applicable",
    "desc": "應為監管、接管、勒令停業清理或命令解散之處分",
    "expr": [
      "EQ",
      "supervisory_measures_applicable",
      [
        "OR",
        [
          "EQ",
          "capital_level_4_noncompliance",
          true
        ],
        [
          "AND",
          [
            "EQ",
            "significant_deterioration",
            true
          ],
          [
            "EQ",
            "improvement_plan_submitted_and_approved",
            true
          ],
          [
            "EQ",
            "improvement_plan_accelerated_deterioration",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:prohibited_acts_without_supervisor_consent",
    "desc": "監管處分期間未經監管人同意之禁止行為",
    "expr": [
      "EQ",
      "prohibited_acts_without_supervisor_consent",
      [
        "OR",
        [
          "GT",
          "payment_exceeding_limit",
          0.0
        ],
        [
          "EQ",
          "contract_or_major_commitment_made",
          true
        ],
        [
          "EQ",
          "other_major_financial_impact",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_business_scope",
    "desc": "違反第一百三十八條相關業務範圍規定",
    "expr": [
      "EQ",
      "violation_business_scope",
      [
        "EQ",

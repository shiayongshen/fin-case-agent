[
  {
    "id": "insurance:capital_level",
    "desc": "保險業資本等級分類（1=資本適足, 2=資本不足, 3=資本顯著不足, 4=資本嚴重不足, 0=未分類）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "OR",
          [
            "AND",
            [
              "GE",
              "capital_adequacy_ratio",
              150.0
            ],
            [
              "LT",
              "capital_adequacy_ratio",
              200.0
            ]
          ],
          [
            "AND",
            [
              "LT",
              "net_worth_ratio",
              3.0
            ],
            [
              "GE",
              "net_worth_ratio",
              2.0
            ]
          ]
        ],
        2,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            200.0
          ],
          [
            "OR",
            [
              "GE",
              "net_worth_ratio",
              3.0
            ],
            [
              "GE",
              "net_worth_ratio_prev",
              3.0
            ]
          ]
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_lowest_rule",
    "desc": "資本等級以較低等級為準",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "LT",
          "capital_adequacy_ratio",
          50.0
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient",
    "desc": "資本嚴重不足等級判定",
    "expr": [
      "EQ",
      "capital_severely_insufficient",
      [
        "EQ",
        "capital_level",
        4
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient",
    "desc": "資本顯著不足等級判定",
    "expr": [
      "EQ",
      "capital_significantly_insufficient",
      [
        "EQ",
        "capital_level",
        3
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient",
    "desc": "資本不足等級判定",
    "expr": [
      "EQ",
      "capital_insufficient",
      [
        "EQ",
        "capital_level",
        2
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_adequate",
    "desc": "資本適足等級判定",
    "expr": [
      "EQ",
      "capital_adequate",
      [
        "EQ",
        "capital_level",
        1
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_completed",
    "desc": "增資、財務或業務改善計畫或合併已於主管機關規定期限內完成",
    "expr": [
      "EQ",
      "improvement_plan_completed",
      [
        "EQ",
        "improvement_plan_done",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_overdue",
    "desc": "增資、財務或業務改善計畫或合併未於主管機關規定期限內完成",
    "expr": [
      "EQ",
      "improvement_plan_overdue",
      [
        "NOT",
        [
          "EQ",
          "improvement_plan_completed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severe_insufficient_and_no_improvement",
    "desc": "資本嚴重不足且未依主管機關規定期限完成增資、財務或業務改善計畫或合併",
    "expr": [
      "EQ",
      "capital_severe_insufficient_and_no_improvement",
      [
        "AND",
        [
          "EQ",
          "capital_severely_insufficient",
          true
        ],
        [
          "EQ",
          "improvement_plan_overdue",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:financial_or_business_deterioration",
    "desc": "財務或業務狀況顯著惡化，不能支付債務或無法履行契約責任或有損及被保險人權益之虞",
    "expr": [
      "EQ",
      "financial_or_business_deterioration",
      [
        "EQ",
        "financial_or_business_deterioration_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_approved",
    "desc": "主管機關核定財務或業務改善計畫",
    "expr": [
      "EQ",
      "improvement_plan_approved",
      [
        "EQ",
        "improvement_plan_approved_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:loss_or_net_worth_accelerated_deterioration",
    "desc": "損益、淨值呈現加速惡化",
    "expr": [
      "EQ",
      "loss_or_net_worth_accelerated_deterioration",
      [
        "EQ",
        "loss_or_net_worth_accelerated_deterioration_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_not_effective",
    "desc": "經輔導仍未改善，致仍有財務或業務惡化之虞",
    "expr": [
      "EQ",
      "improvement_plan_not_effective",
      [
        "EQ",
        "improvement_plan_not_effective_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:regulatory_action_required",
    "desc": "應為監管、接管、勒令停業清理或命令解散之處分",
    "expr": [
      "EQ",
      "regulatory_action_required",
      [
        "OR",
        [
          "EQ",
          "capital_severe_insufficient_and_no_improvement",
          true
        ],
        [
          "AND",
          [
            "NOT",
            [
              "EQ",
              "capital_severe_insufficient_and_no_improvement",
              true
            ]
          ],
          [
            "EQ",
            "financial_or_business_deterioration",
            true
          ],
          [
            "EQ",
            "improvement_plan_approved",
            true
          ],
          [
            "OR",
            [
              "EQ",
              "loss_or_net_worth_accelerated_deterioration",
              true
            ],
            [
              "EQ",
              "improvement_plan_not_effective",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_internal_control",
    "desc": "未建立或未執行內部控制或稽核制度",
    "expr": [
      "EQ",
      "violation_internal_control",
      [
        "NOT",
        [
          "EQ",
          "internal_control_established_and_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_internal_handling",
    "desc": "未建立或未執行內部處理制度或程序",
    "expr": [
      "EQ",
      "violation_internal_handling",
      [
        "NOT",
        [
          "EQ",
          "internal_handling_established_and_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_internal_control_and_handling",
    "desc": "違反內部控制或內部處理制度規定",
    "expr": [
      "EQ",
      "violation_internal_control_and_handling",
      [
        "OR",
        [
          "EQ",
          "violation_internal_control",
          true
        ],
        [
          "EQ",
          "violation_internal_handling",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：違反內部控制或內部處理制度規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "violation_internal_control",
          true
        ],
        [
          "EQ",
          "violation_internal_handling",
          true
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
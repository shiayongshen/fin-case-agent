[
  {
    "id": "bank:violation_of_law_or_regulation",
    "desc": "銀行違反法令、章程或有礙健全經營之虞",
    "expr": [
      "EQ",
      "violation_of_law_or_regulation",
      true
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "bank:corrective_action_ordered",
    "desc": "主管機關已予以糾正並命其限期改善",
    "expr": [
      "EQ",
      "corrective_action_ordered",
      true
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "bank:penalty_measures",
    "desc": "主管機關依情節輕重採取處分措施",
    "expr": [
      "EQ",
      "penalty_measures",
      [
        "OR",
        [
          "EQ",
          "revoke_statutory_meeting_resolution",
          true
        ],
        [
          "EQ",
          "suspend_partial_business",
          true
        ],
        [
          "EQ",
          "restrict_investment",
          true
        ],
        [
          "EQ",
          "order_or_prohibit_asset_disposition",
          true
        ],
        [
          "EQ",
          "order_branch_closure",
          true
        ],
        [
          "EQ",
          "order_manager_staff_dismissal_or_suspension",
          true
        ],
        [
          "EQ",
          "order_director_supervisor_dismissal_or_suspension",
          true
        ],
        [
          "EQ",
          "order_provision_reserve_fund",
          true
        ],
        [
          "EQ",
          "other_necessary_measures",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "bank:notify_registration_authority",
    "desc": "解除董事、監察人職務時通知公司登記主管機關撤銷或廢止其登記",
    "expr": [
      "EQ",
      "notify_registration_authority",
      [
        "IMPLIES",
        [
          "EQ",
          "order_director_supervisor_dismissal_or_suspension",
          true
        ],
        [
          "EQ",
          "notification_sent_to_registration_authority",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "bank:business_guidance_needed",
    "desc": "為改善營運缺失有業務輔導之必要",
    "expr": [
      "EQ",
      "business_guidance_needed",
      true
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "bank:business_guidance_assigned",
    "desc": "主管機關指定機構辦理業務輔導",
    "expr": [
      "EQ",
      "business_guidance_assigned",
      [
        "IMPLIES",
        [
          "EQ",
          "business_guidance_needed",
          true
        ],
        [
          "EQ",
          "business_guidance_assigned",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "bank"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 1,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：銀行違反法令章程或有礙健全經營且未限期改善時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "AND",
        [
          "EQ",
          "violation_of_law_or_regulation",
          true
        ],
        [
          "NOT",
          [
            "EQ",
            "corrective_action_ordered",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "meta"
  }
]
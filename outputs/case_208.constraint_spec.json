[
  {
    "id": "insurance:capital_level",
    "desc": "保險業資本等級分類（1=適足, 2=不足, 3=顯著不足, 4=嚴重不足）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_lower_priority",
    "desc": "資本等級以較低等級為準",
    "expr": [
      "EQ",
      "capital_level_lower_priority",
      [
        "CASE",
        [
          "LE",
          "capital_level",
          "capital_level_other"
        ],
        "capital_level",
        "capital_level_other"
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient_measures_executed",
    "desc": "資本嚴重不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_severely_insufficient_measures_executed",
      [
        "EQ",
        "capital_severely_insufficient_measures_completed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient_measures_executed",
    "desc": "資本顯著不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_significantly_insufficient_measures_executed",
      [
        "EQ",
        "capital_significantly_insufficient_measures_completed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures_executed",
    "desc": "資本不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_insufficient_measures_executed",
      [
        "AND",
        [
          "EQ",
          "improvement_plan_submitted",
          true
        ],
        [
          "EQ",
          "improvement_plan_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient",
    "desc": "資本嚴重不足判定",
    "expr": [
      "EQ",
      "capital_severely_insufficient",
      [
        "EQ",
        "capital_level",
        4
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient",
    "desc": "資本顯著不足判定",
    "expr": [
      "EQ",
      "capital_significantly_insufficient",
      [
        "EQ",
        "capital_level",
        3
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient",
    "desc": "資本不足判定",
    "expr": [
      "EQ",
      "capital_insufficient",
      [
        "EQ",
        "capital_level",
        2
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_adequate",
    "desc": "資本適足判定",
    "expr": [
      "EQ",
      "capital_adequate",
      [
        "EQ",
        "capital_level",
        1
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：資本嚴重不足且未完成增資或改善計畫，或資本等級不足且未執行對應措施時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "AND",
          [
            "EQ",
            "capital_level",
            4
          ],
          [
            "OR",
            [
              "NOT",
              [
                "EQ",
                "capital_severely_insufficient_measures_executed",
                true
              ]
            ],
            [
              "NOT",
              [
                "EQ",
                "capital_increase_completed",
                true
              ]
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level",
            3
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_significantly_insufficient_measures_executed",
              true
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level",
            2
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_insufficient_measures_executed",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
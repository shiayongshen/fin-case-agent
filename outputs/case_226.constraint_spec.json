[
  {
    "id": "securities:violation_of_law",
    "desc": "證券商董事、監察人及受僱人違反證券交易法或相關法令，影響業務正常執行",
    "expr": [
      "EQ",
      "violation_of_law",
      [
        "EQ",
        "violation_detected",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:penalty_applicable",
    "desc": "主管機關可命令停止一年以下業務或解除職務，並依情節輕重處分",
    "expr": [
      "EQ",
      "penalty_applicable",
      [
        "EQ",
        "violation_of_law",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:honesty_and_credit_compliance",
    "desc": "證券商負責人及業務人員執行業務應本誠實及信用原則",
    "expr": [
      "EQ",
      "honesty_and_credit_compliance",
      [
        "EQ",
        "honesty_and_credit_principle_followed",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_1",
    "desc": "以職務上所知悉消息從事上市或上櫃有價證券買賣以獲取投機利益",
    "expr": [
      "EQ",
      "prohibited_behavior_1",
      [
        "NOT",
        [
          "EQ",
          "insider_trading_for_speculation",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_2",
    "desc": "非依法令查詢洩漏客戶委託事項及職務上所獲悉秘密",
    "expr": [
      "EQ",
      "prohibited_behavior_2",
      [
        "NOT",
        [
          "EQ",
          "illegal_disclosure_of_client_info",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_3",
    "desc": "受理客戶對買賣有價證券之種類、數量、價格及買進或賣出之全權委託",
    "expr": [
      "EQ",
      "prohibited_behavior_3",
      [
        "NOT",
        [
          "EQ",
          "full_discretionary_trading_accepted",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_4",
    "desc": "對客戶作贏利保證或分享利益之證券買賣",
    "expr": [
      "EQ",
      "prohibited_behavior_4",
      [
        "NOT",
        [
          "EQ",
          "profit_guarantee_or_sharing",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_5",
    "desc": "約定與客戶共同承擔買賣有價證券之交易損益",
    "expr": [
      "EQ",
      "prohibited_behavior_5",
      [
        "NOT",
        [
          "EQ",
          "shared_trading_loss_profit",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_6",
    "desc": "接受客戶委託買賣有價證券時，同時以自己計算為買入或賣出相對行為",
    "expr": [
      "EQ",
      "prohibited_behavior_6",
      [
        "NOT",
        [
          "EQ",
          "self_dealing_in_client_trades",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_7",
    "desc": "利用客戶名義或帳戶申購、買賣有價證券",
    "expr": [
      "EQ",
      "prohibited_behavior_7",
      [
        "NOT",
        [
          "EQ",
          "using_client_account_for_trading",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_8",
    "desc": "以他人或親屬名義供客戶申購、買賣有價證券",

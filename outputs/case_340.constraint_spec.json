[
  {
    "id": "insurance:capital_level",
    "desc": "資本等級分類（1=適足, 2=不足, 3=顯著不足, 4=嚴重不足, 0=未分類）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            0.5
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            0.5
          ],
          [
            "LT",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            1.5
          ],
          [
            "LT",
            "net_worth_ratio_prev1",
            0.02
          ],
          [
            "GE",
            "net_worth_ratio_prev2",
            0.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            1.5
          ],
          [
            "LT",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            2.0
          ],
          [
            "OR",
            [
              "GE",
              "net_worth_ratio_prev1",
              0.02
            ],
            [
              "GE",
              "net_worth_ratio_prev2",
              0.03
            ]
          ]
        ],
        2,
        [
          "AND",
          [
            "GE",
            [
              "DIV",
              "own_capital",
              "risk_capital"
            ],
            2.0
          ],
          [
            "OR",
            [
              "GE",
              "net_worth_ratio_prev1",
              0.03
            ],
            [
              "GE",
              "net_worth_ratio_prev2",
              0.03
            ]
          ]
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 1,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：資本嚴重不足且未依主管機關規定期限完成增資、改善計畫或合併時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "AND",
        [
          "EQ",
          "capital_level",
          4
        ],
        [
          "NOT",
          [
            "OR",
            [
              "EQ",
              "capital_increase_completed",
              true
            ],
            [
              "EQ",
              "financial_or_business_improvement_plan_completed",
              true
            ],
            [
              "EQ",
              "merger_completed",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "meta"
  }
]
                100.0
              ],
              100.0
            ],
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "OR",
          [
            "AND",
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "capital_adequacy_ratio",
                  100.0
                ],
                100.0
              ],
              50.0
            ],
            [
              "LT",
              [
                "MUL",
                [
                  "DIV",
                  "capital_adequacy_ratio",
                  100.0
                ],
                100.0
              ],
              150.0
            ]
          ],
          [
            "AND",
            [
              "LT",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio",
                  100.0
                ],
                100.0
              ],
              2.0
            ],
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio",
                  100.0
                ],
                100.0
              ],
              0.0
            ]
          ]
        ],
        3,
        [
          "OR",
          [
            "AND",
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "capital_adequacy_ratio",
                  100.0
                ],
                100.0
              ],
              150.0
            ],
            [
              "LT",
              [
                "MUL",
                [
                  "DIV",
                  "capital_adequacy_ratio",
                  100.0
                ],
                100.0
              ],
              200.0
            ]
          ],
          [
            "AND",
            [
              "LT",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio",
                  100.0
                ],
                100.0
              ],
              3.0
            ],
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio",
                  100.0
                ],
                100.0
              ],
              2.0
            ]
          ]
        ],
        2,
        [
          "AND",
          [
            "GE",
            [
              "MUL",
              [
                "DIV",
                "capital_adequacy_ratio",
                100.0
              ],
              100.0
            ],
            200.0
          ],
          [
            "OR",
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio",
                  100.0
                ],
                100.0
              ],
              3.0
            ],
            [
              "GE",
              [
                "MUL",
                [
                  "DIV",
                  "net_worth_ratio_prev",
                  100.0
                ],
                100.0
              ],
              3.0
            ]
          ]
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_ok",
    "desc": "已建立且執行內部控制及稽核制度",
    "expr": [
      "EQ",
      "internal_control_ok",
      [
        "AND",
        [
          "EQ",
          "internal_control_established",
          true
        ],
        [
          "EQ",
          "internal_control_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_handling_ok",
    "desc": "已建立且執行內部處理制度及程序",
    "expr": [
      "EQ",
      "internal_handling_ok",
      [
        "AND",
        [
          "EQ",
          "internal_handling_established",
          true
        ],
        [
          "EQ",
          "internal_handling_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 1,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：未建立或未執行內部控制及稽核制度或內部處理制度及程序時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "internal_control_ok",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_handling_ok",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "meta"
  }
]
[
  {
    "id": "insurance:fixed_office_and_account_book",
    "desc": "保險代理人、經紀人、公證人應有固定業務處所並專設帳簿記載業務收支",
    "expr": [
      "EQ",
      "fixed_office_and_account_book_ok",
      [
        "AND",
        [
          "EQ",
          "has_fixed_office",
          true
        ],
        [
          "EQ",
          "has_dedicated_account_book",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:single_license_only",
    "desc": "兼有保險代理人、經紀人、公證人資格者僅得擇一申領執業證照",
    "expr": [
      "EQ",
      "single_license_only_ok",
      [
        "LE",
        [
          "ADD",
          [
            "IF",
            [
              "EQ",
              "is_agent",
              true
            ],
            1,
            0
          ],
          [
            "IF",
            [
              "EQ",
              "is_broker",
              true
            ],
            1,
            0
          ],
          [
            "IF",
            [
              "EQ",
              "is_notary",
              true
            ],
            1,
            0
          ]
        ],
        1
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_required",
    "desc": "保險代理人公司、經紀人公司為公開發行公司或具一定規模者，應建立內部控制、稽核制度與招攬處理制度及程序",
    "expr": [
      "EQ",
      "internal_control_required_ok",
      [
        "IMPLIES",
        [
          "OR",
          [
            "EQ",
            "is_publicly_listed",
            true
          ],
          [
            "EQ",
            "has_certain_scale",
            true
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "internal_control_established",
            true
          ],
          [
            "EQ",
            "audit_system_established",
            true
          ],
          [
            "EQ",
            "solicitation_handling_system_established",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_executed",
    "desc": "保險代理人公司、經紀人公司應確實執行內部控制、稽核制度與招攬處理制度及程序",
    "expr": [
      "EQ",
      "internal_control_executed_ok",
      [
        "IMPLIES",
        [
          "OR",
          [
            "EQ",
            "is_publicly_listed",
            true
          ],
          [
            "EQ",
            "has_certain_scale",
            true
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "internal_control_executed",
            true
          ],
          [
            "EQ",
            "audit_system_executed",
            true
          ],
          [
            "EQ",
            "solicitation_handling_system_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_compliance",
    "desc": "符合內部控制建立及執行規定",
    "expr": [
      "EQ",
      "internal_control_compliance",
      [
        "AND",
        [
          "EQ",
          "internal_control_required_ok",
          true
        ],
        [
          "EQ",
          "internal_control_executed_ok",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:article_165_3_violation",
    "desc": "違反保險法第165條第三項規定未建立或未確實執行內部控制、稽核制度、招攬處理制度或程序",
    "expr": [
      "EQ",
      "article_165_3_violation",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "internal_control_established",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "audit_system_established",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "solicitation_handling_system_established",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_control_executed",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "audit_system_executed",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "solicitation_handling_system_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "personal_data:collection_purpose_valid",
    "desc": "非公務機關蒐集或處理個人資料應有特定目的且符合第19條規定之一",
    "expr": [
      "EQ",
      "collection_purpose_valid",
      [
        "OR",
        [
          "EQ",
          "legal_provision",
          true
        ],
        [
          "AND",
          [
            "EQ",
            "contract_relation",
            true
          ],
          [
            "EQ",
            "appropriate_security_measures",
            true
          ]
        ],
        [
          "EQ",
          "publicly_disclosed_data",
          true
        ],
        [
          "AND",
          [
            "EQ",
            "academic_research_public_interest",
            true
          ],
          [
            "EQ",
            "data_anonymized",
            true
          ]
        ],
        [
          "EQ",
          "subject_consent",
          true
        ],
        [
          "EQ",
          "necessary_for_public_interest",
          true
        ],
        [
          "NOT",
          [
            "EQ",
            "subject_prohibits_processing",
            true
          ]
        ],
        [
          "EQ",
          "no_harm_to_subject_rights",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:processing_prohibition_compliance",
    "desc": "蒐集或處理者知悉或經當事人通知禁止處理或利用時，應刪除、停止處理或利用該個人資料",
    "expr": [
      "EQ",
      "processing_prohibition_compliance",
      [
        "IMPLIES",
        [
          "EQ",
          "processing_prohibition_notified",
          true
        ],
        [
          "EQ",
          "processing_stopped_or_deleted",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:use_within_purpose",
    "desc": "非公務機關利用個人資料應於蒐集特定目的必要範圍內",
    "expr": [
      "EQ",
      "use_within_purpose_ok",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "is_non_public_agency",
            true
          ]
        ],
        [
          "EQ",
          "use_within_collection_purpose",
          true
        ],
        [
          "EQ",
          "legal_provision_for_use_outside_purpose",
          true
        ],
        [
          "EQ",
          "necessary_for_public_interest_use",
          true
        ],
        [
          "EQ",
          "prevent_life_or_property_danger",
          true
        ],
        [
          "EQ",
          "prevent_major_harm_to_others",
          true
        ],
        [
          "AND",
          [
            "EQ",
            "public_agency_or_academic",
            true
          ],
          [
            "EQ",
            "data_anonymized",
            true
          ]
        ],
        [
          "EQ",
          "subject_consent_for_use",
          true
        ],
        [
          "EQ",
          "benefit_subject",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:marketing_refusal_handling",
    "desc": "非公務機關行銷時，當事人表示拒絕接受行銷時應停止利用其個人資料行銷",
    "expr": [
      "EQ",
      "marketing_refusal_handling_ok",
      [
        "IMPLIES",
        [
          "AND",
          [
            "EQ",
            "is_non_public_agency",
            true
          ],
          [
            "EQ",
            "marketing_refusal_received",
            true
          ]
        ],
        [
          "EQ",
          "marketing_stopped",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:marketing_first_notice",
    "desc": "非公務機關首次行銷時應提供拒絕接受行銷方式並支付所需費用",
    "expr": [
      "EQ",
      "marketing_first_notice_ok",
      [
        "IMPLIES",
        [
          "AND",
          [
            "EQ",
            "is_non_public_agency",
            true
          ],
          [
            "EQ",
            "is_first_marketing",
            true
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "provided_rejection_method",
            true
          ],
          [
            "EQ",
            "paid_required_fee",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:article_19_violation",
    "desc": "違反個人資料保護法第19條規定",
    "expr": [
      "EQ",
      "article_19_violation",
      [
        "NOT",
        [
          "EQ",
          "collection_purpose_valid",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:article_20_violation",
    "desc": "違反個人資料保護法第20條第一項規定",
    "expr": [
      "EQ",
      "article_20_violation",
      [
        "NOT",
        [
          "EQ",
          "use_within_purpose_ok",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "personal_data:article_47_violation",
    "desc": "違反個人資料保護法第47條規定",
    "expr": [
      "EQ",
      "article_47_violation",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "article_6_1_compliance",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "article_19_violation",
            false
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "article_20_violation",
            false
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "international_transfer_restriction_compliance",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "privacy"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 1,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：違反保險法第165條第三項或個人資料保護法相關規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "article_165_3_violation",
          true
        ],
        [
          "EQ",
          "article_19_violation",
          true
        ],
        [
          "EQ",
          "article_20_violation",
          true
        ],
        [
          "EQ",
          "article_47_violation",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "meta"
  }
]
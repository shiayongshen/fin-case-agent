[
  {
    "id": "insurance:capital_level",
    "desc": "資本等級分類（1=適足, 2=不足, 3=顯著不足, 4=嚴重不足）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_severely_insufficient",
    "desc": "資本等級為嚴重不足",
    "expr": [
      "EQ",
      "capital_level_severely_insufficient",
      [
        "EQ",
        "capital_level",
        4
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_significantly_insufficient",
    "desc": "資本等級為顯著不足",
    "expr": [
      "EQ",
      "capital_level_significantly_insufficient",
      [
        "EQ",
        "capital_level",
        3
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_insufficient",
    "desc": "資本等級為不足",
    "expr": [
      "EQ",
      "capital_level_insufficient",
      [
        "EQ",
        "capital_level",
        2
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_completed",
    "desc": "增資、財務或業務改善計畫或合併已於主管機關規定期限內完成",
    "expr": [
      "EQ",
      "improvement_plan_completed",
      [
        "EQ",
        "improvement_plan_submitted_and_completed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_overdue",
    "desc": "增資、財務或業務改善計畫或合併未於主管機關規定期限內完成",
    "expr": [
      "EQ",
      "improvement_plan_overdue",
      [
        "NOT",
        [
          "EQ",
          "improvement_plan_completed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_4_and_plan_overdue",
    "desc": "資本嚴重不足且未於期限內完成增資或改善計畫",
    "expr": [
      "EQ",
      "capital_level_4_and_plan_overdue",
      [
        "AND",
        [
          "EQ",
          "capital_level_severely_insufficient",
          true
        ],
        [
          "EQ",
          "improvement_plan_overdue",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:financial_or_business_deterioration",
    "desc": "財務或業務狀況顯著惡化，不能支付債務或無法履行契約責任或有損及被保險人權益之虞",
    "expr": [
      "EQ",
      "financial_or_business_deterioration",
      [
        "OR",
        [
          "EQ",
          "significant_financial_deterioration",
          true
        ],
        [
          "EQ",
          "unable_to_pay_debt",
          true
        ],
        [
          "EQ",
          "unable_to_fulfill_contract",
          true
        ],
        [
          "EQ",
          "risk_to_insured_rights",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_approved",
    "desc": "主管機關核定財務或業務改善計畫",
    "expr": [
      "EQ",
      "improvement_plan_approved",
      [
        "EQ",
        "improvement_plan_approved_by_authority",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:accelerated_deterioration_or_no_improvement",
    "desc": "損益、淨值加速惡化或經輔導仍未改善",
    "expr": [
      "EQ",
      "accelerated_deterioration_or_no_improvement",
      [
        "OR",
        [
          "EQ",
          "accelerated_loss_or_net_worth_decline",
          true
        ],
        [
          "EQ",
          "no_improvement_after_guidance",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:supervisory_measures_required",
    "desc": "應為監管、接管、勒令停業清理或命令解散之處分",
    "expr": [
      "EQ",
      "supervisory_measures_required",
      [
        "OR",
        [
          "EQ",
          "capital_level_4_and_plan_overdue",
          true
        ],
        [
          "AND",
          [
            "NOT",
            [
              "EQ",
              "capital_level_4_and_plan_overdue",
              true
            ]
          ],
          [
            "EQ",
            "financial_or_business_deterioration",
            true
          ],
          [
            "EQ",
            "improvement_plan_approved",
            true
          ],
          [
            "EQ",
            "accelerated_deterioration_or_no_improvement",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:supervisory_measures_executed",
    "desc": "主管機關已對保險業為監管、接管、勒令停業清理或命令解散之處分",
    "expr": [
      "EQ",
      "supervisory_measures_executed",
      [
        "EQ",
        "supervisory_measures_issued",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_148_1_or_2",
    "desc": "違反第一百四十八條之一第一項或第二項規定",
    "expr": [
      "EQ",
      "violation_148_1_or_2",
      [
        "EQ",
        "violation_148_1_or_2_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_148_2_1",
    "desc": "違反第一百四十八條之二第一項規定，未提供說明文件或文件不實",
    "expr": [
      "EQ",
      "violation_148_2_1",
      [
        "EQ",
        "violation_148_2_1_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_148_2_2",
    "desc": "違反第一百四十八條之二第二項規定，未依限報告或公開說明或內容不實",
    "expr": [
      "EQ",
      "violation_148_2_2",
      [
        "EQ",
        "violation_148_2_2_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_148_3_1",
    "desc": "違反第一百四十八條之三第一項規定，未建立或未執行內部控制或稽核制度",
    "expr": [
      "EQ",
      "violation_148_3_1",
      [
        "EQ",
        "violation_148_3_1_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_148_3_2",
    "desc": "違反第一百四十八條之三第二項規定，未建立或未執行內部處理制度或程序",
    "expr": [
      "EQ",
      "violation_148_3_2",
      [
        "EQ",
        "violation_148_3_2_flag",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 1,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：違反相關法令規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "violation_148_1_or_2",
          true
        ],
        [
          "EQ",
          "violation_148_2_1",
          true
        ],
        [
          "EQ",
          "violation_148_2_2",
          true
        ],
        [
          "EQ",
          "violation_148_3_1",
          true
        ],
        [
          "EQ",
          "violation_148_3_2",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "meta"
  }
]
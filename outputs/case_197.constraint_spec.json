[
  {
    "id": "insurance:capital_adequacy_ratio",
    "desc": "資本適足率",
    "expr": [
      "EQ",
      "capital_adequacy_ratio",
      "capital_adequacy_ratio"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:net_worth_ratio",
    "desc": "淨值比率",
    "expr": [
      "EQ",
      "net_worth_ratio",
      "net_worth_ratio"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:net_worth",
    "desc": "淨值",
    "expr": [
      "EQ",
      "net_worth",
      "net_worth"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level",
    "desc": "資本等級分類（1=適足, 2=不足, 3=顯著不足, 4=嚴重不足）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "OR",
          [
            "AND",
            [
              "GE",
              "capital_adequacy_ratio",
              150.0
            ],
            [
              "LT",
              "capital_adequacy_ratio",
              200.0
            ]
          ],
          [
            "AND",
            [
              "LT",
              "net_worth_ratio",
              3.0
            ],
            [
              "LT",
              "net_worth_ratio",
              2.0
            ]
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_corrected",
    "desc": "資本等級修正（以較低等級為準）",
    "expr": [
      "EQ",
      "capital_level_corrected",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_submitted",
    "desc": "增資、財務或業務改善計畫已提出",
    "expr": [
      "EQ",
      "improvement_plan_submitted",
      "improvement_plan_submitted"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:improvement_plan_executed",
    "desc": "增資、財務或業務改善計畫已執行",
    "expr": [
      "EQ",
      "improvement_plan_executed",
      "improvement_plan_executed"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures",
    "desc": "資本不足等級措施執行完成",
    "expr": [
      "EQ",
      "capital_insufficient_measures",
      [
        "AND",
        [
          "EQ",
          "improvement_plan_submitted",
          true
        ],
        [
          "EQ",
          "improvement_plan_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient_measures",
    "desc": "資本顯著不足等級措施執行完成",
    "expr": [
      "EQ",
      "capital_significantly_insufficient_measures",
      [
        "EQ",
        "significantly_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient_measures",
    "desc": "資本嚴重不足等級措施執行完成",
    "expr": [
      "EQ",
      "capital_severely_insufficient_measures",
      [
        "EQ",
        "severely_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：資本不足等級達一定程度且未執行對應措施時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "AND",
          [
            "EQ",
            "capital_level_corrected",
            4
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_severely_insufficient_measures",
              true
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level_corrected",
            3
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_significantly_insufficient_measures",
              true
            ]
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "capital_level_corrected",
            2
          ],
          [
            "NOT",
            [
              "EQ",
              "capital_insufficient_measures",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
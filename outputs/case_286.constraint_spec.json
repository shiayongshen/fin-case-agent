          true
        ],
        6,
        [
          "EQ",
          "remove_director_supervisor",
          true
        ],
        7,
        [
          "EQ",
          "order_provision_reserve",
          true
        ],
        8,
        [
          "EQ",
          "other_necessary_measures",
          true
        ],
        9,
        0
      ]
    ],
    "weight": 1,
    "domain": "bank"
  },

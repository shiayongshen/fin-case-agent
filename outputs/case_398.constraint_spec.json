[
  {
    "id": "insurance:agent_broker_notary_license_and_guarantee",
    "desc": "保險代理人、經紀人、公證人須經主管機關許可，繳存保證金並投保相關保險，領有執業證照後始得經營或執行業務",
    "expr": [
      "EQ",
      "agent_broker_notary_license_and_guarantee",
      [
        "AND",
        [
          "EQ",
          "license_permitted",
          true
        ],
        [
          "EQ",
          "guarantee_deposit_paid",
          true
        ],
        [
          "EQ",
          "related_insurance_purchased",
          true
        ],
        [
          "EQ",
          "practice_certificate_held",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:related_insurance_type",
    "desc": "相關保險種類依身份區分",
    "expr": [
      "EQ",
      "related_insurance_type",
      [
        "CASE",
        [
          "EQ",
          "role_agent",
          true
        ],
        "liability_insurance",
        [
          "EQ",
          "role_notary",
          true
        ],
        "liability_insurance",
        [
          "EQ",
          "role_broker",
          true
        ],
        "liability_and_guarantee_insurance",
        "none"
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:minimum_amount_and_implementation",
    "desc": "繳存保證金及投保相關保險之最低金額及實施方式由主管機關定之",
    "expr": [
      "EQ",
      "minimum_amount_and_implementation",
      [
        "EQ",
        "authority_defined_minimum_and_implementation",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:qualification_and_management_rules",
    "desc": "資格取得、申請許可條件、程序、文件及管理規則由主管機關定之",
    "expr": [
      "EQ",
      "qualification_and_management_rules",
      [
        "EQ",
        "authority_defined_qualification_and_management_rules",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:bank_permitted_to_operate_agent_or_broker",
    "desc": "銀行得經主管機關許可擇一兼營保險代理人或保險經紀人業務，並分別準用相關規定",
    "expr": [
      "EQ",
      "bank_permitted_to_operate_agent_or_broker",
      [
        "AND",
        [
          "EQ",
          "bank_authority_permitted",
          true
        ],
        [
          "OR",
          [
            "EQ",
            "bank_operate_agent",
            true
          ],
          [
            "EQ",
            "bank_operate_broker",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:broker_duty_of_care_and_fidelity",
    "desc": "保險經紀人應以善良管理人注意義務及忠實義務為被保險人洽訂保險契約或提供服務",
    "expr": [
      "EQ",
      "broker_duty_of_care_and_fidelity",
      [
        "AND",
        [
          "EQ",
          "broker_exercise_duty_of_care",
          true
        ],
        [
          "EQ",
          "broker_exercise_fidelity",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:broker_provide_written_report_and_disclose_fee",
    "desc": "保險經紀人洽訂保險契約前，於主管機關指定範圍內，應主動提供書面分析報告，收取報酬者應明確告知報酬標準",
    "expr": [
      "EQ",
      "broker_provide_written_report_and_disclose_fee",
      [
        "AND",
        [
          "EQ",
          "broker_provide_written_report",
          true
        ],
        [
          "IMPLIES",
          [
            "EQ",
            "broker_charge_fee",
            true
          ],
          [
            "EQ",
            "broker_disclose_fee_standard",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:authority_define_report_scope_and_fee_standard",
    "desc": "主管機關定書面分析報告適用範圍、內容及報酬收取標準範圍",
    "expr": [
      "EQ",
      "authority_define_report_scope_and_fee_standard",
      [
        "EQ",
        "authority_defined_report_scope_and_fee_standard",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：未經主管機關許可、未繳存保證金、未投保相關保險或未領執業證照時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "license_permitted",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "guarantee_deposit_paid",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "related_insurance_purchased",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "practice_certificate_held",
            true
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
[
  {
    "id": "insurance:compulsory_insurance_benefit_items",
    "desc": "強制汽車責任保險給付項目包含傷害醫療費用、失能給付及死亡給付",
    "expr": [
      "EQ",
      "compulsory_insurance_benefit_items",
      [
        "AND",
        [
          "EQ",
          "injury_medical_expense_covered",
          true
        ],
        [
          "EQ",
          "disability_benefit_covered",
          true
        ],
        [
          "EQ",
          "death_benefit_covered",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:underwriting_and_claim_recording_compliance",
    "desc": "保險人應正確記載承保資料及辦理理賠",
    "expr": [
      "EQ",
      "underwriting_and_claim_recording_compliance",
      [
        "AND",
        [
          "EQ",
          "underwriting_data_correctly_recorded",
          true
        ],
        [
          "EQ",
          "claims_handled_properly",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_and_handling_system_established_and_executed",
    "desc": "保險業建立並執行內部控制及稽核制度與內部處理制度及程序",
    "expr": [
      "EQ",
      "internal_control_and_handling_system_established_and_executed",
      [
        "AND",
        [
          "EQ",
          "internal_control_and_audit_system_established",
          true
        ],
        [
          "EQ",
          "internal_control_and_audit_system_executed",
          true
        ],
        [
          "EQ",
          "internal_handling_system_established",
          true
        ],
        [
          "EQ",
          "internal_handling_system_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：違反承保資料記載、理賠辦理、內部控制及內部處理制度規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "underwriting_and_claim_recording_compliance",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_control_and_handling_system_established_and_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "taiwan_china:business_direct_transaction_permitted",
    "desc": "臺灣地區金融保險證券期貨機構及其境外分支機構經財政部許可得與大陸地區人民及機構有業務直接往來",
    "expr": [
      "EQ",
      "business_direct_transaction_permitted",
      [
        "EQ",
        "financial_institution_permitted_by_mof",
        true
      ]
    ],
    "weight": 1,
    "domain": "taiwan_china"
  },
  {
    "id": "taiwan_china:branch_establishment_permitted",
    "desc": "臺灣地區金融保險證券期貨機構在大陸地區設立分支機構應報經財政部許可",
    "expr": [
      "EQ",
      "branch_establishment_permitted",
      [
        "EQ",
        "branch_establishment_approved_by_mof",
        true
      ]
    ],
    "weight": 1,
    "domain": "taiwan_china"
  },
  {
    "id": "taiwan_china:mof_restriction_order_enforced",
    "desc": "財政部得報請行政院核定後限制或禁止業務直接往來",
    "expr": [
      "EQ",
      "mof_restriction_order_enforced",
      [
        "EQ",
        "mof_restriction_order_active",
        true
      ]
    ],
    "weight": 1,
    "domain": "taiwan_china"
  },
  {
    "id": "taiwan_china:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "taiwan_china:penalty_conditions",
    "desc": "處罰條件：違反業務直接往來許可、分支機構設立許可或違反限制命令時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "business_direct_transaction_permitted",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "branch_establishment_permitted",
            true
          ]
        ],
        [
          "AND",
          [
            "EQ",
            "mof_restriction_order_enforced",
            true
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
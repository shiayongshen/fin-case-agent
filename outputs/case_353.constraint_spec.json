[
  {
    "id": "insurance:reserve_calculation_compliance",
    "desc": "保險業於營業年度屆滿時，應分別保險種類計算應提存各種準備金並記載於特設帳簿",
    "expr": [
      "EQ",
      "reserve_calculation_compliance",
      [
        "AND",
        [
          "EQ",
          "reserve_calculated_by_insurance_type",
          true
        ],
        [
          "EQ",
          "reserve_recorded_in_special_books",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:violation_144_145",
    "desc": "違反保險法第144條第一項至第四項或第145條規定",
    "expr": [
      "EQ",
      "violation_144_145",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "compliance_144_1_to_4",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "reserve_calculation_compliance",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_control_established_and_executed",
    "desc": "建立並執行內部控制及稽核制度",
    "expr": [
      "EQ",
      "internal_control_established_and_executed",
      [
        "AND",
        [
          "EQ",
          "internal_control_established",
          true
        ],
        [
          "EQ",
          "internal_control_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:internal_handling_established_and_executed",
    "desc": "建立並執行內部處理制度及程序",
    "expr": [
      "EQ",
      "internal_handling_established_and_executed",
      [
        "AND",
        [
          "EQ",
          "internal_handling_established",
          true
        ],
        [
          "EQ",
          "internal_handling_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：違反第144條第1至4項或第145條規定時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "violation_144_145",
          true
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_control_established_and_executed",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "internal_handling_established_and_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
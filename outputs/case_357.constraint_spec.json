[
  {
    "id": "insurance:capital_level",
    "desc": "資本等級分類（1=適足, 2=不足, 3=顯著不足, 4=嚴重不足）",
    "expr": [
      "EQ",
      "capital_level",
      [
        "CASE",
        [
          "OR",
          [
            "LT",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "net_worth",
            0.0
          ]
        ],
        4,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            50.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "GE",
            "net_worth_ratio",
            0.0
          ],
          [
            "LT",
            "net_worth_ratio",
            2.0
          ]
        ],
        3,
        [
          "AND",
          [
            "GE",
            "capital_adequacy_ratio",
            150.0
          ],
          [
            "LT",
            "capital_adequacy_ratio",
            200.0
          ]
        ],
        2,
        [
          "GE",
          "capital_adequacy_ratio",
          200.0
        ],
        1,
        0
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_level_lower_priority",
    "desc": "資本等級以較低等級為準",
    "expr": [
      "EQ",
      "capital_level",
      "capital_level"
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_severely_insufficient_measures_executed",
    "desc": "資本嚴重不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_severely_insufficient_measures_executed",
      [
        "EQ",
        "severely_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_significantly_insufficient_measures_executed",
    "desc": "資本顯著不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_significantly_insufficient_measures_executed",
      [
        "EQ",
        "significantly_insufficient_measures_executed",
        true
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:capital_insufficient_measures_executed",
    "desc": "資本不足等級措施已執行",
    "expr": [
      "EQ",
      "capital_insufficient_measures_executed",
      [
        "AND",
        [
          "EQ",
          "improvement_plan_submitted",
          true
        ],
        [
          "EQ",
          "improvement_plan_executed",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:serious_insufficient_and_no_measures",
    "desc": "資本嚴重不足且未完成增資、改善計畫或合併",
    "expr": [
      "EQ",
      "serious_insufficient_and_no_measures",
      [
        "AND",
        [
          "EQ",
          "capital_level",
          4
        ],
        [
          "NOT",
          [
            "OR",
            [
              "EQ",
              "capital_severely_insufficient_measures_executed",
              true
            ],
            [
              "EQ",
              "capital_significantly_insufficient_measures_executed",
              true
            ],
            [
              "EQ",
              "capital_insufficient_measures_executed",
              true
            ]
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:financial_or_business_deterioration",
    "desc": "財務或業務狀況顯著惡化且未改善",
    "expr": [
      "EQ",
      "financial_or_business_deterioration",
      [
        "AND",
        [
          "EQ",
          "financial_or_business_deteriorated",
          true
        ],
        [
          "NOT",
          [
            "EQ",
            "improvement_plan_executed",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：資本嚴重不足且未完成增資、改善計畫或合併，或財務業務顯著惡化且未改善時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "EQ",
          "serious_insufficient_and_no_measures",
          true
        ],
        [
          "EQ",
          "financial_or_business_deterioration",
          true
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  }
]
[
  {
    "id": "insurance:license_required",
    "desc": "保險代理人、經紀人、公證人須經主管機關許可",
    "expr": [
      "EQ",
      "license_required",
      [
        "AND",
        [
          "EQ",
          "agent_license_granted",
          true
        ],
        [
          "EQ",
          "broker_license_granted",
          true
        ],
        [
          "EQ",
          "notary_license_granted",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:deposit_guarantee_and_insurance_required",
    "desc": "須繳存保證金並投保相關保險",
    "expr": [
      "EQ",
      "deposit_and_insurance_ok",
      [
        "AND",
        [
          "GE",
          "guarantee_deposit_amount",
          "guarantee_deposit_minimum"
        ],
        [
          "EQ",
          "agent_liability_insurance",
          true
        ],
        [
          "EQ",
          "notary_liability_insurance",
          true
        ],
        [
          "EQ",
          "broker_liability_insurance",
          true
        ],
        [
          "EQ",
          "broker_guarantee_insurance",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:license_and_deposit_and_insurance_ok",
    "desc": "領有執業證照且繳存保證金並投保相關保險後，始得經營或執行業務",
    "expr": [
      "EQ",
      "license_and_deposit_and_insurance_ok",
      [
        "AND",
        [
          "EQ",
          "license_required",
          true
        ],
        [
          "EQ",
          "deposit_and_insurance_ok",
          true
        ],
        [
          "EQ",
          "practice_certificate_held",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "insurance"
  },
  {
    "id": "insurance:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "insurance:penalty_conditions",
    "desc": "處罰條件：未經主管機關許可、未繳存保證金或未投保相關保險，或未持有執業證照者處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "license_required",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "deposit_and_insurance_ok",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "practice_certificate_held",
            true
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]
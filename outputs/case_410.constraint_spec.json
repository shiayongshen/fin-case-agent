[
  {
    "id": "securities:violation_penalty_level",
    "desc": "違反證券投資信託及顧問法之處分等級（1=警告,2=解除職務,3=停止募集或新增業務,4=停業,5=廢止營業許可,6=其他處置,0=無處分）",
    "expr": [
      "EQ",
      "violation_penalty_level",
      [
        "CASE",
        [
          "EQ",
          "violation_warning",
          true
        ],
        1,
        [
          "EQ",
          "violation_dismiss_officer",
          true
        ],
        2,
        [
          "EQ",
          "violation_suspend_fund_or_business",
          true
        ],
        3,
        [
          "EQ",
          "violation_suspend_operation",
          true
        ],
        4,
        [
          "EQ",
          "violation_revoke_license",
          true
        ],
        5,
        [
          "EQ",
          "violation_other_measures",
          true
        ],
        6,
        0
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:personnel_violation",
    "desc": "董事、監察人、經理人或受僱人違反法令且影響業務正常執行",
    "expr": [
      "EQ",
      "personnel_violation",
      [
        "EQ",
        "personnel_violation_occurred",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:personnel_suspension_or_dismissal",
    "desc": "主管機關命令停止執行業務或解除職務",
    "expr": [
      "EQ",
      "personnel_suspension_or_dismissal",
      [
        "OR",
        [
          "EQ",
          "order_suspend_business_within_one_year",
          true
        ],
        [
          "EQ",
          "order_dismiss_personnel",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:violation_fine_and_improvement",
    "desc": "違反第111條規定且罰鍰及限期改善",
    "expr": [
      "EQ",
      "violation_fine_and_improvement",
      [
        "EQ",
        "violation_111_occurred",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:violation_111_repeat_fine",
    "desc": "第111條違規屢次未改善，罰鍰加倍",
    "expr": [
      "EQ",
      "violation_111_repeat_fine",
      [
        "AND",
        [
          "EQ",
          "violation_111_occurred",
          true
        ],
        [
          "EQ",
          "violation_111_not_improved",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:fiduciary_duty_compliance",
    "desc": "負責人及業務人員遵守善良管理人注意義務及忠實義務",
    "expr": [
      "EQ",
      "fiduciary_duty_compliance",
      [
        "EQ",
        "duty_of_care_and_loyalty_observed",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behaviors",
    "desc": "未有證券投資信託事業負責人與業務人員管理規則第13條禁止行為",
    "expr": [
      "EQ",
      "no_prohibited_behaviors",
      [
        "AND",
        [
          "NOT",
          [
            "EQ",
            "behavior_leak_info",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_self_dealing",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_false_or_fraud",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_not_return_commission",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_offer_special_benefit",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_transfer_proxy_vote_for_benefit",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_manipulate_market_price",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_change_account_without_reason",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_public_promotion_or_forecast",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_unreasonable_commission",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_agent_for_others",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "behavior_other_harmful",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:confidentiality_compliance",
    "desc": "遵守保守秘密義務",
    "expr": [
      "EQ",
      "confidentiality_compliance",
      [
        "EQ",
        "confidentiality_observed",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:restricted_trading_prohibition",
    "desc": "負責人等及其關係人不得從事特定股票及衍生商品交易",
    "expr": [
      "EQ",
      "restricted_trading_prohibition",
      [
        "NOT",
        [
          "EQ",
          "restricted_trading_occurred",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:restricted_trading_reporting",
    "desc": "負責人等及其關係人應申報特定股票及衍生商品交易",
    "expr": [
      "EQ",
      "restricted_trading_reporting",
      [
        "EQ",
        "restricted_trading_reported",
        true
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:prohibited_behavior_full_trust_investment",
    "desc": "全權委託投資業務禁止行為",
    "expr": [
      "EQ",
      "no_prohibited_behavior_full_trust",
      [
        "AND",
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_use_info_for_others",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_harm_client",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_profit_loss_sharing",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_self_dealing",
            true
          ]
        ],
        [
          "OR",
          [
            "NOT",
            [
              "EQ",
              "full_trust_behavior_relative_trading",
              true
            ]
          ],
          [
            "AND",
            [
              "EQ",
              "full_trust_behavior_relative_trading",
              true
            ],
            [
              "EQ",
              "full_trust_relative_trading_not_intentional",
              true
            ]
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_use_client_account",
            true
          ]
        ],
        [
          "OR",
          [
            "NOT",
            [
              "EQ",
              "full_trust_behavior_subcontract_or_transfer",
              true
            ]
          ],
          [
            "EQ",
            "full_trust_behavior_subcontract_or_transfer_exempted",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_change_account_without_reason",
            true
          ]
        ],
        [
          "OR",
          [
            "NOT",
            [
              "EQ",
              "full_trust_behavior_no_reason_investment_decision",
              true
            ]
          ],
          [
            "EQ",
            "full_trust_behavior_reasonable_explanation",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "full_trust_behavior_other_harmful",
            true
          ]
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "securities:restricted_trading_prohibition_full_trust",
    "desc": "全權委託投資業務負責人等及其關係人不得從事特定股票及衍生商品交易",
    "expr": [
      "EQ",
      "restricted_trading_prohibition_full_trust",
      [
        "NOT",
        [
          "EQ",
          "restricted_trading_full_trust_occurred",
          true
        ]
      ]
    ],
    "weight": 1,
    "domain": "securities"
  },
  {
    "id": "meta:penalty_default_false",
    "desc": "預設不處罰",
    "expr": [
      "EQ",
      "penalty",
      false
    ],
    "weight": 0,
    "domain": "meta"
  },
  {
    "id": "meta:penalty_conditions",
    "desc": "處罰條件：違反任一法令規定或管理規則禁止行為時處罰",
    "expr": [
      "EQ",
      "penalty",
      [
        "OR",
        [
          "NOT",
          [
            "EQ",
            "no_prohibited_behaviors",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "fiduciary_duty_compliance",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "confidentiality_compliance",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "restricted_trading_prohibition",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "restricted_trading_reporting",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "no_prohibited_behavior_full_trust",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "restricted_trading_prohibition_full_trust",
            true
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "violation_fine_and_improvement",
            false
          ]
        ],
        [
          "NOT",
          [
            "EQ",
            "personnel_violation",
            false
          ]
        ]
      ]
    ],
    "weight": 0,
    "domain": "meta"
  }
]